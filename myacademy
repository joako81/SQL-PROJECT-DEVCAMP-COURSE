CREATE SCHEMA bbdd_academy;

USE bb_academy;


CREATE TABLE `bbdd_academy`.`students` (
  `students_id` INT NOT NULL AUTO_INCREMENT,
  `students_fullname` VARCHAR(45) NOT NULL,
  `student_email` VARCHAR(45) NULL,
  PRIMARY KEY (`students_id`),
  UNIQUE INDEX `students_id_UNIQUE` (`students_id` ASC) VISIBLE,
  UNIQUE INDEX `student_email_UNIQUE` (`student_email` ASC) VISIBLE);